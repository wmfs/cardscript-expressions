{
  "title": "Feedback cleaning demo!",
  "widgets": [
    {
      "type": "header",
      "attributes": {
        "heading": "All done!",
        "desc": "We're all done here, we would really appreciate some feedback though!"
      }
    },
    {
      "id": "name",
      "type": "textarea",
      "attributes": {
        "heading": "Your Name"
      }
    },
    {
      "id": "userWantsToGiveFeedback",
      "type": "switch",
      "attributes": {
        "default": false,
        "heading": "I would like to leave some feedback"
      }
    },
    {
      "id": "feedbackSet",
      "type": "set",
      "showWhen": "data.userWantsToGiveFeedback",
      "attributes": {
        "tocTitle": "Feedback!"
      }
    },
    {
      "id": "feedback",
      "type": "switch",
      "attributes": {
        "heading": "Is this the best app you've ever seen?",
        "desc": "How did you feel using our app?"
      }
    },
    {
      "id": "reasonSet",
      "type": "set",
      "showWhen": "data.feedback",
      "attributes": {
        "tocTitle": "Reasons!"
      }
    },
    {
      "id": "feedbackReason",
      "type": "textarea",
      "attributes": {
        "heading": "Why is it so good?"
      }
    },
    {
      "type": "endSet"
    },
    {
      "id": "useAgain",
      "type": "switch",
      "attributes": {
        "heading": "Would you use this app again?"
      }
    },
    {
      "type": "endSet"
    }
  ]
}
