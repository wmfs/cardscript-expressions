{
  "name": "@wmfs/cardscript-cleaner",
  "description": "Cleans Cardscript form data to ensure we only submit the data that we should.",
  "version": "0.0.0-semantically-released",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "esprima": "4.0.1",
    "static-eval": "2.0.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.1.0",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "semantic-release": "15.13.17",
    "standard": "12.0.1",
    "@semantic-release/changelog": "3.0.1",
    "@semantic-release/git": "7.0.13",
    "@wmfs/cardscript-examples": "1.0.0"
  },
  "scripts": {
    "generate": "node ./lib",
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-expressions.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-expressions/issues"
  },
